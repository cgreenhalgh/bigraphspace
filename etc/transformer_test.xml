<?xml version="1.0" encoding="UTF-8"?>
<tests>
	<!-- first test - into a room -->
	<test>
		<redex>
			<bigraph>
				<root>
					<PhysicalHandheld _support="_PH1">
						<site index="0" />
					</PhysicalHandheld>
					<PhysicalRoom _support="_PR1">
						<site index="1" />
					</PhysicalRoom>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<PhysicalRoom _support="_PR1">
						<PhysicalHandheld _support="_PH1">
							<site index="0" />
						</PhysicalHandheld>
						<site index="1" />
					</PhysicalRoom>
				</root>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<PhysicalHandheld _support="PH1"></PhysicalHandheld>
					<PhysicalRoom _support="PR1">
						<PhysicalHandheld _support="PH2"></PhysicalHandheld>
					</PhysicalRoom>
					<PhysicalHandheld _support="PH3"></PhysicalHandheld>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<PhysicalRoom _support="PR1">
						<PhysicalHandheld _support="PH1"></PhysicalHandheld>
						<PhysicalHandheld _support="PH2"></PhysicalHandheld>
					</PhysicalRoom>
					<PhysicalHandheld _support="PH3"></PhysicalHandheld>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- copy test -->
	<test>
		<redex>
			<bigraph>
				<root>
					<PhysicalHandheld _support="_PH1">
						<site index="0" />
					</PhysicalHandheld>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<PhysicalHandheld _support="_PH1">
						<site index="0" />
					</PhysicalHandheld>
					<PhysicalHandheld _support="_PH1">
						<site index="0" />
					</PhysicalHandheld>
				</root>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<PhysicalHandheld _support="PH1"></PhysicalHandheld>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<PhysicalHandheld _support="PH1"></PhysicalHandheld>
					<PhysicalHandheld _support="PH1"></PhysicalHandheld>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- first link-preserving test -->
	<test>
		<redex>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
					</DescriptionRoom>
					<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
						<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1"/>
					<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- link present in context and parameter -->
	<test>
		<redex>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
					</DescriptionRoom>
					<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
						<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers1"/>
					</DescriptionRoom>
					<DescriptionBluetooth _support="DB3" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers1"/>
					</DescriptionRoom>
					<DescriptionBluetooth _support="DB3" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- break link in parameter -->
	<test>
		<redex>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
					</DescriptionRoom>
					<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers">
						<site index="0" />
						<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers"/>
					</DescriptionRoom>
				</root>
				<innername name="_identifiers" id=""/>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers1"/>
					</DescriptionRoom>
					<DescriptionBluetooth _support="DB3" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers2"/>
					</DescriptionRoom>
					<DescriptionBluetooth _support="DB3" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- join existing links -->
	<test>
		<redex>
			<bigraph>
				<root>
					<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers1"/>
					<DescriptionRoom _support="_DR1" identifier="_identifiers2">
						<site index="0" />
					</DescriptionRoom>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers1"/>
					<DescriptionRoom _support="_DR1" identifier="_identifiers1">
						<site index="0" />
					</DescriptionRoom>
				</root>
				<innername name="_identifiers2" id="_identifiers1"/>
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
					<DescriptionRoom _support="DR1" identifier="identifiers2">
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers2"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB2" associatedwith="identifiers1"/>
					</DescriptionRoom>
					<DescriptionBluetooth _support="DB1" associatedwith="identifiers1"/>
				</root>
			</bigraph>
		</result>
	</test>
	<!-- make simple link -->
	<test>
		<redex>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers1">
						<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers2"/>
						<site index="0" />
					</DescriptionRoom>
				</root>
			</bigraph>
		</redex>
		<reactum>
			<bigraph>
				<root>
					<DescriptionRoom _support="_DR1" identifier="_identifiers3">
						<DescriptionBluetooth _support="_DB1" associatedwith="_identifiers3"/>
						<site index="0" />
					</DescriptionRoom>
				</root>
				<!-- <innername name="_identifiers2" id="_identifiers1"/> -->
			</bigraph>
		</reactum>
		<target>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers1">
						<DescriptionBluetooth _support="DB1" associatedwith="identifiers2"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</target>
		<result>
			<bigraph>
				<root>
					<DescriptionRoom _support="DR1" identifier="identifiers3">
						<DescriptionBluetooth _support="DB1" associatedwith="identifiers3"/>
					</DescriptionRoom>
				</root>
			</bigraph>
		</result>
	</test>
</tests>
