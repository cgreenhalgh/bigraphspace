		<pattern>
			<bigraph>
				<root>
					<site index="0" />
				</root>
				<root>
					<site index="1" />
				</root>
			</bigraph>
		</pattern>
		<target>
			<bigraph>
				<root>
					<PhysicalHandheld _support="PH1"></PhysicalHandheld>
				</root>
				<root>
					<PhysicalHandheld _support="PH2"></PhysicalHandheld>
				</root>
			</bigraph>
		</target>
		<match>
			<!--  <linkmatch />  -->
		</match>
