	<!-- 17. Integer exact match -->
	<test>
		<pattern>
			<bigraph>
				<root>
					<integer _support="n">
						<index>10</index>
					</integer>
				</root>
			</bigraph>
		</pattern>
		<target>
			<bigraph>
				<root>
					<integer _support="10">
						<index>10</index>
					</integer>
					<integer _support="11">
						<index>11</index>
					</integer>
				</root>
			</bigraph>
		</target>
		<match>
			<nodematch pattern="n" target="10"/>
		</match>
	</test>
	<!-- 18. Integer variable match -->
	<test>
		<pattern>
			<bigraph>
				<variable name="x" type="integer"/>
				<root>
					<integer _support="n">
						<index variable="x"/>
					</integer>
				</root>
			</bigraph>
		</pattern>
		<target>
			<bigraph>
				<root>
					<integer _support="10">
						<index>10</index>
					</integer>
					<integer _support="11">
						<index>11</index>
					</integer>
				</root>
			</bigraph>
		</target>
		<match>
			<nodematch pattern="n" target="10"/>
			<variable pattern="x" target="10"/>
		</match>
		<match>
			<nodematch pattern="n" target="11"/>
			<variable pattern="x" target="11"/>
		</match>
	</test>
	<!-- 19. Common value Integer variable match -->
	<test>
		<pattern>
			<bigraph>
				<variable name="x" type="integer"/>
				<root>
					<integer _support="n">
						<index variable="x"/>
					</integer>
					<PhysicalRoom _support="PR1">
						<integer _support="Rn">
							<index variable="x"/>
						</integer>
					</PhysicalRoom>
				</root>
			</bigraph>
		</pattern>
		<target>
			<bigraph>
				<root>
					<integer _support="10">
						<index>10</index>
					</integer>
					<integer _support="11">
						<index>11</index>
					</integer>
					<PhysicalRoom _support="PR1">
						<integer _support="R10">
							<index>10</index>
						</integer>
						<integer _support="R11">
							<index>11</index>
						</integer>
					</PhysicalRoom>
				</root>
			</bigraph>
		</target>
		<match>
			<nodematch pattern="n" target="10"/>
			<nodematch pattern="Rn" target="R10"/>
			<nodematch pattern="PR1" target="PR1"/>
			<variable pattern="x" target="10"/>
		</match>
		<match>
			<nodematch pattern="n" target="11"/>
			<nodematch pattern="Rn" target="R11"/>
			<nodematch pattern="PR1" target="PR1"/>
			<variable pattern="x" target="11"/>
		</match>
	</test>
