					<constraint type="maxvalue">12</constraint>
				</variable>
				<root>
					<integer _support="n">
						<index variable="x"/>
					</integer>
				</root>
			</bigraph>
		</pattern>
		<target>
			<bigraph>
				<root>
					<integer _support="10">
						<index>10</index>
					</integer>
					<integer _support="11">
						<index>11</index>
					</integer>
					<integer _support="12">
						<index>12</index>
					</integer>
					<integer _support="13">
						<index>13</index>
					</integer>
				</root>
			</bigraph>
		</target>
		<match>
			<nodematch pattern="n" target="11"/>
			<variable pattern="x" target="11"/>
		</match>
		<match>
			<nodematch pattern="n" target="12"/>
			<variable pattern="x" target="12"/>
		</match>
	</test>
