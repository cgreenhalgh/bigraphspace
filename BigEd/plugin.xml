<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.5"?>
<plugin>
    <extension
          point="org.eclipse.core.contenttype.contentTypes">
   <content-type base-type="org.eclipse.core.runtime.xml"
                 file-extensions="xml"
                 id="bigraphXML"
                 name="Bigraph XML File"
                 priority="normal">
      <describer class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber2">
         <parameter name="element"
                    value="bigraph">
         </parameter>
      </describer>
   </content-type>
   <content-type
         file-extensions="btl"
         id="bigraphBTL"
         name="Bigraph Text File"
         priority="normal">
   </content-type>
   <content-type
         base-type="org.eclipse.core.runtime.xml"
         file-extensions="xml"
         id="bigraphSignature"
         name="Bigraph Signature XML"
         priority="normal">
      <describer
            class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber2">
         <parameter
               name="element"
               value="{http://www.mrl.nott.ac.uk/bigraphs/signature/1.0}definitions">
         </parameter>
      </describer>
   </content-type>
    </extension>

    <extension
          point="org.eclipse.ui.editors">
       <editor
             class="bigraph.biged.ui.editors.BigraphEditor"
             contributorClass="bigraph.biged.ui.editors.BigraphEditorActionBarContributor"
             default="true"
             id="bigraph.biged.editors.BigraphEditor"
             name="BigEd">
          <contentTypeBinding
                contentTypeId="bigraph.biged.bigraphXML">
          </contentTypeBinding>
          <contentTypeBinding
                contentTypeId="bigraph.biged.bigraphBTL">
          </contentTypeBinding>
       </editor>
       <editor
             class="bigraph.biged.ui.editors.BigraphSignatureEditor"
             contributorClass="bigraph.biged.ui.editors.BigraphSignatureEditorContributor"
             default="true"
             id="bigraph.biged.ui.editors.BigraphSignatureEditor"
             name="Bigraph Signature Editor">
          <contentTypeBinding
                contentTypeId="bigraph.biged.bigraphSignature">
          </contentTypeBinding>
       </editor>
    </extension>
    <extension
          point="org.eclipse.ui.newWizards">
       <category
             id="bigraph.biged"
             name="Bigraph">
       </category>
       <wizard
             category="bigraph.biged"
             class="bigraph.biged.ui.wizards.NewBigraphXML"
             hasPages="true"
             icon="icons/sample.gif"
             id="bigraph.biged.ui.wizards.NewBigraphXML"
             name="Bigraph XML"
             project="false">
       </wizard>
       <wizard
             category="bigraph.biged"
             class="bigraph.biged.ui.wizards.NewBigraphBTL"
             hasPages="true"
             icon="icons/sample.gif"
             id="bigraph.biged.ui.wizards.NewBigraphBTL"
             name="Bigraph BTL"
             project="false">
       </wizard>       
    </extension>
    <extension
          point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
       <propertyContributor
             contributorId="bigraph.biged.ui.properties.contributor"
             labelProvider="bigraph.biged.ui.BigraphLabelProvider"
             typeMapper="bigraph.biged.ui.properties.TypeMapper">
          <propertyCategory
                category="bigraph"></propertyCategory>
       </propertyContributor>
    </extension>
    <extension
          point="org.eclipse.ui.views.properties.tabbed.propertySections">
       <propertySections
             contributorId="bigraph.biged.ui.properties.contributor">
          <propertySection
                class="bigraph.biged.ui.properties.BigraphSignatureSection"
                enablesFor="1"
                id="bigraph.biged.ui.properties.BigraphSignatureSection"
                tab="bigraph.biged.ui.properties.general">
             <input
                   type="bigraph.biged.model.Bigraph">
             </input>
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.EdgePortSection"
                enablesFor="1"
                id="bigraph.biged.ui.properties.EdgePortSection"
                tab="bigraph.biged.ui.properties.general">
             <input
                   type="bigraph.biged.model.Edge">
             </input>
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.PlacePortListSection"
                enablesFor="1"
                filter="bigraph.biged.ui.properties.NodeFilter"
                id="bigraph.biged.ui.properties.PlacePortListSection"
                tab="bigraph.biged.ui.properties.general">
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.PlaceIndexesSection"
                enablesFor="1"
                filter="bigraph.biged.ui.properties.NodeFilter"
                id="bigraph.biged.ui.properties.PlaceIndexesSection"
                tab="bigraph.biged.ui.properties.general">
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.PlaceSupportNameSection"
                enablesFor="1"
                filter="bigraph.biged.ui.properties.NodeFilter"
                id="bigraph.biged.ui.properties.PlaceSupportNameSection"
                tab="bigraph.biged.ui.properties.general">
          </propertySection>          
          <propertySection
                class="bigraph.biged.ui.properties.PlaceControlSection"
                enablesFor="1"
                filter="bigraph.biged.ui.properties.NodeFilter"
                id="bigraph.biged.ui.properties.PlaceControlSection"
                tab="bigraph.biged.ui.properties.general">
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.SiteIndexSection"
                enablesFor="1"
                filter="bigraph.biged.ui.properties.SiteFilter"
                id="bigraph.biged.ui.properties.SiteIndexSection"
                tab="bigraph.biged.ui.properties.general">
          </propertySection>  
                      <propertySection
                            class="bigraph.biged.ui.properties.PortEdgeNameSection"
                            enablesFor="1"
                            id="bigraph.biged.ui.properties.PortEdgeNameSection"
                            tab="bigraph.biged.ui.properties.general">
             <input
                   type="bigraphspace.model.Port">
             </input>                
          </propertySection>
          <propertySection
                class="bigraph.biged.ui.properties.PortNameSection"
                enablesFor="1"
                id="bigraph.biged.ui.properties.PortNameSection"
                tab="bigraph.biged.ui.properties.general">
             <input
                   type="bigraphspace.model.Port">
             </input>                                
          </propertySection>          
       </propertySections>
    </extension>
    <extension
          point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
       <propertyTabs
             contributorId="bigraph.biged.ui.properties.contributor">
          <propertyTab
                category="bigraph"
                id="bigraph.biged.ui.properties.general"
                label="General">
          </propertyTab>
       </propertyTabs>
    </extension>
</plugin>
